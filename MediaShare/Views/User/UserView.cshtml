@model MediaShare.ViewModels.UserList
@{
    ViewBag.Title = "UserView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>User List</h1>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Username</th>
        <th>Password</th>
    </tr>
    @foreach (var i in Model.Users)
    {
        <tr>
            <td>@i.Name</td>
            <td>@i.Username</td>
            <td>@i.Password</td>
        </tr>
    }

</table>

